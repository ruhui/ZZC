<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/windowbackcolor">

    <com.zzcar.zzc.views.widget.NavBar2_
        android:id="@+id/mNavbar"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        android:gravity="bottom"
        android:background="@color/white"
        app:title_res="@string/tab_mine">
    </com.zzcar.zzc.views.widget.NavBar2_>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/windowbackcolor"/>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">


        <TextView
            android:id="@+id/textView162"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="昵称"
            android:textColor="#2e2d2d"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:id="@+id/textView163"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView162"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            android:text=""
            android:textColor="#2e2d2d"
            android:textSize="@dimen/sp_14" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/windowbackcolor"
        android:layout_marginLeft="@dimen/size_15"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">

        <TextView
            android:id="@+id/textView164"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:text="店铺"
            android:textSize="@dimen/sp_14"
            android:textColor="#2e2d2d"/>

        <TextView
            android:id="@+id/textView165"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView164"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            android:text=""
            android:textSize="@dimen/sp_14"
            android:textColor="#2e2d2d"/>
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:text="保证金"
        android:textSize="@dimen/sp_14"
        android:textColor="#2e2d2d"
        android:gravity="center|left"
        android:paddingLeft="15dp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">

        <TextView
            android:id="@+id/txtSecuery"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:background="@color/white"
            android:textSize="@dimen/sp_14"
            android:textColor="#2e2d2d"
            android:text="3000元"
            android:gravity="center|left"
            android:paddingLeft="15dp"/>

        <ImageView
            android:id="@+id/imageView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            app:srcCompat="@drawable/nav_icon_selected" />

    </RelativeLayout>



    <TextView
        android:id="@+id/textView166"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="保证金缴纳后将在钱包中冻结"
        android:gravity="center|left"
        android:paddingLeft="15dp"
        android:textSize="@dimen/sp_14"
        android:textColor="#212121"/>


    <RelativeLayout
        android:id="@+id/relaSelect"
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nav_icon_default"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:id="@+id/imgSelt"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="我已经阅读并同意签约"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imgSelt"
            android:layout_toEndOf="@+id/imgSelt"
            android:layout_marginLeft="10dp"
            android:textSize="14sp"
            android:textColor="#212121"
            android:id="@+id/txtXieyi" />

    </RelativeLayout>


    <TextView
        android:id="@+id/textView102"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/txtXieyi"
        android:layout_toEndOf="@+id/txtXieyi"
        android:layout_toRightOf="@+id/txtXieyi"
        android:text="查看《众众车交易担保店铺使用协议》"
        android:textSize="14sp"
        android:gravity="center"
        android:textColor="#212121"/>

    <TextView
        android:id="@+id/txtSubmit"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="申请开通"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/white"
        android:gravity="center"
        android:background="@drawable/shape_red_radio"
        android:layout_marginLeft="@dimen/size_15"
        android:layout_marginRight="@dimen/size_15"
        android:layout_marginTop="@dimen/size_15"/>

</LinearLayout>