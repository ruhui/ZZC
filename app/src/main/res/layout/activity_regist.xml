<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.zzcar.zzc.views.widget.NavBar2_
        android:id="@+id/mNavbar"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        android:gravity="bottom"
        android:background="@color/white"
        app:title_res="@string/tab_mine">
    </com.zzcar.zzc.views.widget.NavBar2_>

    <EditText
        android:id="@+id/editText12"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="请填写昵称"
        android:textSize="14sp"
        android:paddingLeft="@dimen/size_15"
        android:textColor="@color/color_333333"
        android:background="@color/white"
        android:layout_marginTop="15dp"
        android:inputType="textPersonName"
        android:text="" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/grey"/>

    <EditText
        android:id="@+id/editText13"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:ems="10"
        android:paddingLeft="@dimen/size_15"
        android:hint="请填写手机号码"
        android:background="@color/white"
        android:textSize="14sp"
        android:textColor="@color/color_333333"
        android:inputType="phone" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/grey"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:layout_below="@+id/view6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout9">

        <EditText
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:inputType="numberDecimal"
            android:text=""
            android:ems="10"
            android:background="@null"
            android:textSize="14sp"
            android:hint="请填写手机收到的验证码"
            android:layout_marginLeft="15dp"
            android:textColor="@color/color_333333"
            android:id="@+id/editText7"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@+id/textView91"
            android:layout_toStartOf="@+id/textView91" />

        <TextView
            android:text="获取验证码"
            android:layout_width="127dp"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/textView91"
            android:textSize="15sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:background="@color/app_red"
            />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/grey"/>

    <EditText
        android:id="@+id/editText14"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:ems="10"
        android:paddingLeft="15dp"
        android:textSize="14sp"
        android:background="@color/white"
        android:textColor="@color/color_333333"
        android:hint="请填写登录密码"
        android:inputType="textPassword" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/grey"/>

    <EditText
        android:id="@+id/editText15"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:ems="10"
        android:paddingLeft="15dp"
        android:textSize="14sp"
        android:background="@color/white"
        android:textColor="@color/color_333333"
        android:hint="请填写确认密码"
        android:inputType="textPassword" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nav_icon_default"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:id="@+id/imgSelt"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="我已经阅读并同意"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imgSelt"
            android:layout_toEndOf="@+id/imgSelt"
            android:layout_marginLeft="10dp"
            android:textSize="12sp"
            android:textColor="#212121"
            android:id="@+id/txtXieyi" />

        <TextView
            android:id="@+id/textView102"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/txtXieyi"
            android:layout_toEndOf="@+id/txtXieyi"
            android:layout_toRightOf="@+id/txtXieyi"
            android:text="注册服务协议"
            android:textSize="12sp"
            android:textColor="@color/app_red"
            android:layout_marginLeft="10dp"/>

    </RelativeLayout>

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="注册"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/white"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/shape_red_radio"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/textView103"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="113dp"
            android:layout_marginStart="113dp"
            android:textSize="12sp"
            android:textColor="@color/color_333333"
            android:text="我已有账号" />

        <Button
            android:id="@+id/button5"
            android:layout_width="60dp"
            android:layout_height="22dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/textView103"
            android:layout_toRightOf="@+id/textView103"
            android:text="马上登录"
            android:layout_marginLeft="20dp"
            android:background="@drawable/shape_grey_radio"
            android:textSize="12sp"
            android:textColor="@color/color_333333"/>
    </RelativeLayout>
</LinearLayout>