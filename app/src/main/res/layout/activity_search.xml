<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_search"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zzcar.zzc.activities.SearchActivity">


    <com.zzcar.zzc.views.widget.NavBar2_
        android:id="@+id/mNavbar"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        android:gravity="bottom"
        android:background="@color/white"
        app:title_res="@string/tab_mine">
    </com.zzcar.zzc.views.widget.NavBar2_>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/mNavbar"
        android:background="@color/white"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"/>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon1"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon2"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon3"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon4"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon5"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon6"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <com.zzcar.zzc.views.widget.ItemIconTextIcon_
            android:id="@+id/itemIconTextIcon7"
            android:layout_width="match_parent"
            android:layout_height="50dp">

        </com.zzcar.zzc.views.widget.ItemIconTextIcon_>

        <TextView
            android:id="@+id/sureSubmit"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="确定"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:background="@drawable/shape_red_radio"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:gravity="center"
            />

    </LinearLayout>
</RelativeLayout>
