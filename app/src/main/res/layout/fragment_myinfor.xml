<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/windowbackcolor">

    <com.zzcar.zzc.views.widget.NavBar2_
        android:id="@+id/mNavbar"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        android:gravity="bottom"
        android:background="@color/white"
        app:title_res="@string/tab_mine">
    </com.zzcar.zzc.views.widget.NavBar2_>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/mNavbar">

        <RelativeLayout
            android:id="@+id/relaRenzheng"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/selector_bg_white_grey"
            android:layout_marginTop="5dp">

            <TextView
                android:text="车行认证"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/textView58"
                android:layout_marginLeft="15dp"
                android:textColor="@color/color_090909"
                android:textSize="15sp"/>

            <ImageView
                android:id="@+id/mid_rightImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/nav_icon_right_default"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="15dp"/>

            <TextView
                android:text="未认证"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView58"
                android:layout_toLeftOf="@+id/mid_rightImg"
                android:layout_toStartOf="@+id/mid_rightImg"
                android:id="@+id/textView62"
                android:textSize="15sp"
                android:textColor="@color/color_090909"
                android:layout_marginRight="15dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relaHeagimg"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:background="@drawable/selector_bg_white_grey"
            android:layout_marginTop="5dp">

            <TextView
                android:text="头像"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"
                android:id="@+id/textView63"
                android:textSize="15sp"
                android:textColor="@color/color_090909"/>

            <ImageView
                android:layout_width="38dp"
                android:layout_height="38dp"
                app:srcCompat="@drawable/nav_icon_head_default"
                android:layout_marginRight="15dp"
                android:layout_marginEnd="15dp"
                android:id="@+id/imageView13"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"/>

        <RelativeLayout
            android:id="@+id/relaNick"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:background="@drawable/selector_bg_white_grey">

            <TextView
                android:text="昵称"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView64"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"
                android:textColor="@color/color_090909"
                android:textSize="15sp"/>

            <ImageView
                android:id="@+id/mid_rightImg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/nav_icon_right_default"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="15dp"/>

            <TextView
                android:text=""
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView64"
                android:layout_toLeftOf="@+id/mid_rightImg1"
                android:layout_toStartOf="@+id/mid_rightImg1"
                android:id="@+id/textView65"
                android:textSize="15sp"
                android:gravity="right"
                android:layout_marginRight="@dimen/size_15"
                android:textColor="@color/color_090909"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"/>

        <RelativeLayout
            android:id="@+id/relaTelephone"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:background="@color/white">

            <TextView
                android:text="手机"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"
                android:id="@+id/textView66"
                android:textSize="15sp"
                android:textColor="@color/color_090909"/>

            <TextView
                android:text=" "
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView66"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="15dp"
                android:layout_marginEnd="15dp"
                android:id="@+id/textView67"
                android:gravity="right"
                />
        </RelativeLayout>

    </LinearLayout>


</RelativeLayout>